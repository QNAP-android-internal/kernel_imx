&iomuxc {
	pinctrl_usb_serial_sai_rxd0:usb_serial_sai_rxd0_grp{
		fsl,pins = <
			MX8MM_IOMUXC_SAI5_RXD0_GPIO3_IO21	0x19
		>;	
	};

	pinctrl_usb_serial_sai_rxd1:usb_serial_sai_rxd1_grp{
		fsl,pins = <
			MX8MM_IOMUXC_SAI5_RXD1_GPIO3_IO22	0x19
		>;	
	};

	pinctrl_usb_serial_sai_rxd2:usb_serial_sai_rxd2_grp{
		fsl,pins = <
			MX8MM_IOMUXC_SAI5_RXD2_GPIO3_IO23	0x19
		>;	
	};

	pinctrl_usb2_a:usb2_a_grp{
	       fsl,pins = <
			MX8MM_IOMUXC_GPIO1_IO06_GPIO1_IO6		0x140
