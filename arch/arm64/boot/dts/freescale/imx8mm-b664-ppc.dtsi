&i2c2 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2>;
	status = "okay";

	lightbarsdcardside: tlc59116_left@60 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "ti,tlc59116";
		reg = <0x60>;
		status = "okay";
		out0@0 {
			label = "i2c2_led_red_00";
			reg = <0x0>;
		};

		out1@1 {
			label = "i2c2_led_green_00";
			reg = <0x1>;
		};

		out2@2 {
			label = "i2c2_led_blue_00";
			reg = <0x2>;
		};

		out3@3 {
			label = "i2c2_led_red_01";
			reg = <0x3>;
		};

		out4@4{
			label = "i2c2_led_green_01";
			reg = <0x4>;
		};

		out5@5 {
			label = "i2c2_led_blue_01";
			reg = <0x5>;
		};

		out6@6 {
			label = "i2c2_led_red_02";
			reg = <0x6>;
		};

		out7@7 {
			label = "i2c2_led_green_02";
			reg = <0x7>;
		};

	       out8@8 {
			label = "i2c2_led_blue_02";
			reg = <0x8>;
		};

		out9@9 {
			label = "i2c2_led_red_03";
			reg = <0x9>;
		};

		out10@a {
			label = "i2c2_led_green_03";
			reg = <0xa>;
		};

		out11@b {
			label = "i2c2_led_blue_03";
			reg = <0xb>;
		};

		out12@c {
			label = "i2c2_led_red_04";
			reg = <0xc>;
		};

		out13@d {
			label = "i2c2_led_green_04";
			reg = <0xd>;
		};

		out14@e {
			label = "i2c2_led_blue_04";
			reg = <0xe>;
		};
	};

	lt8911exb@2d {
		compatible = "lontium,lt8911exb";
		reg = <0x2d>;

		lontium,pclk = <75000000>;
		lontium,hact = <1280>;
		lontium,vact = <800>;
		lontium,hbp = <40>;
		lontium,hfp = <40>;
		lontium,vbp = <10>;
		lontium,vfp = <3>;
		lontium,hs = <80>;
		lontium,vs = <10>;

		lontium,mipi_lane = <4>;
		lontium,lane_cnt = <1>;
		lontium,color = <1>; //Color Depth 0:6bit 1:8bit
		lontium,test = <0>;

		status = "okay";
	};
};

